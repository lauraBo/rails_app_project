<head>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

   <script src="javascripts/stockists.js" type="text/javascript>"></script>

  </head>


<h1> Stockists Index </h1>



<% @stockists.each do |stockist| %>
<%= link_to stockist.name, stockist_path(stockist) %></br>
<% end %>

<br>


<%= form_for(@stockist) do |f| %>
<%= f.text_area :name %>
<%= f.text_area :address %>
<p><%= f.submit %></p>
<% end %>
