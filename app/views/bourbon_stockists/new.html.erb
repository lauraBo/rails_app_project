<%= form_for @bourbon_stockist do |f| %>
<%= f.label :notes%>
<%= f.text_field :notes %><br>
   Select Bourbon:
  <%= f.collection_select :bourbon_id, Bourbon.all, :id, :name %>
  Select Stockist:
  <%= f.collection_select :stockist_id, Stockist.all, :id, :name %>
  Submit Note:
  <%= f.submit %>
<% end %>
